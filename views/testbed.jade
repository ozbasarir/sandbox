!!!
html(ng-app="rentalApp", lang='en')
  head
    meta(http-equiv='X-UA-Compatible',content='IE=edge,chrome=1')
    meta(name='viewport',content='width=device-width, initial-scale=1.0')
    title Rental Manager
    link(rel='stylesheet', href='/stylesheets/style.css')
    link(rel='stylesheet', href='/libs/bootstrap/css/bootstrap.min.css')
    link(rel='stylesheet', href='/libs/bootstrap/css/bootstrap-responsive.min.css')
  body
    .container(ng-controller="TabsDemoCtrl", ng-cloak="")
      tabs
        pane(heading="Info")
          p pane1
        pane(heading="Info2")
          p pane2    
        pane(ng-repeat="pane in panes", heading="{{pane.title}}", active="pane.active") {{pane.content}}
    script(src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js")
    script(src="/libs/angular/angular.min.js")
    script(src="/libs/angular/angular-cookies.min.js")
    script(src="/libs/angular/angular-loader.min.js")
    script(src="/libs/angular/angular-resource.min.js")
    script(src="/libs/angular/angular-sanitize.min.js")
    script(src="/libs/bootstrap/js/bootstrap.min.js")
    script(src="/libs/ui-bootstrap-tpls-0.2.0.min.js")
    script(src="/javascripts/testbed.js")
    script(src='/javascripts/services.js')
    script(src='/javascripts/controllers.js')
    script(src='/javascripts/filters.js')
    script(src='/javascripts/directives.js')
